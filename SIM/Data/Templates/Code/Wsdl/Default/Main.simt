<sim-template version="1.0">
  <meta>
    <name>Default</name>
    <description></description>
    <type>SourceCode</type>
    <language>Wsdl</language>
  </meta>
  <files defaultName="{Name}.wsdl">
    <file name="Service.wsdl" outputName="{Diagram.Name}.wsdl" type="Diagram">
      <settings>
        <setting name="DefaultType" value="object"></setting>
        <setting name="NameAdjustment" value="CamelCase"></setting>
      </settings>
    </file>    
  </files>
</sim-template>